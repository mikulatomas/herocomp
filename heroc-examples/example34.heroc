
main ()
{
    /* reference and dereference */
    long a = 888888888, b, c, d;
    b = &a;
    c = &b;
    d = &c;
    print_long (*b);
    print_nl ();
    print_long (**c);
    print_nl ();
    print_long (***d);
    print_nl ();
    print_long (b [0]);
    print_nl ();
    print_long (c [0][0]);
    print_nl ();
    print_long (*c [0]);
    print_nl ();
    print_long (d [0][0][0]);
    print_nl ();
    print_long (*d [0][0]);
    print_nl ();
    print_long (**d [0]);
    print_nl ();
}
